<div id="atras"><%= link_to "atras", root_path %></div>
<h1><center>Sala Deporte</center></h1>

<ul id="chat">
  <%= render @deportes %>
</ul>
<div id="newMsg">
    <%= form_for Deporte.new, :remote => true do |f| %>
      <%= f.text_field :content, :size => 50, :maxlength => 140 %>
      <%= f.submit "Send" %>
    <% end %>
</div>
